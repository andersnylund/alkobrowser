{
    "env": {
        "browser": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "react-hooks"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "react/prop-types": [
            "error"
        ],
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn"
    }
}